/* pages/cloudpic/splash.wxss */

.default{
  background: var(--background);
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  text-align: center;
}
.default .default-image{
  width: 400rpx;
  height: 325rpx;
  margin-top: 220rpx;
}
.bottom{
  text-align: center;
  width: 100%;
  position: fixed;
  bottom: 120rpx;
}
.gobtn{
  width: 250rpx;
  line-height: 60rpx;
  border-radius: 10rpx;
  font-size: 14px;
  text-align: center;
  border: 1px solid var(--border);
  display: inline-block;
  background: #fff;
}