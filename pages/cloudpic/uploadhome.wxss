/* pages/cloudpic/uploadhome.wxss */
page{
  background: var(--background);
}

.top{
  height:420rpx;
}
.top image{
  width: 100%;
  height:420rpx;
}
.middle{
  padding: 0 20rpx 10rpx 40rpx;
}
.middle .title{
  
}
.minitext .num{
  color: #FF5F00;
}
.middle .minitext{
  font-size: 28rpx;
  color: #aaa;
  margin: 10rpx 0 0 0;
  height: 40rpx;
}
.bigtitle{
  height: 80rpx;
  line-height: 90rpx;
  background-color: #fff;
  padding-left: 40rpx;
}

.bottom{
  width: 100%;
  height: 90rpx;
  line-height: 90rpx;
  background-color: #fff;
  text-align: center;
  color: #FF5F00 ;
  position: fixed;
  bottom: 0;
}
.list{
  width:100%;
  margin: 20rpx 15rpx 80rpx 15rpx;
  padding-bottom:20rpx;
}
.image_box{
  display: flex;
 padding: 0 18rpx 0 26rpx;
 flex-wrap:wrap;
}
.avatar{
  width:220rpx;
  height:220rpx;
  padding:3rpx;
}
.mask{
  background: #000;
  position: fixed;
  top: 0;
  height: 100%;
  width: 100%;
}
.mask .close{
  width: 70rpx;
  height: 70rpx;
  line-height: 70rpx;
  background-color: rgba(66, 66, 66, 1);
  border-radius: 50%;
  display: inline-block;
  text-align: center;
  color: #fff;
  font-size: 40rpx;
  margin: 20rpx 20rpx 0 0;
}
.swiper image{
  width: 100%;
  height: auto;
}
.icons{
  position: fixed;
  bottom: 0;
  left: 0;
  color: #fff;
  font-size: 24rpx;
  height: 110rpx;
}
.icons .item{
  display: inline-block;
  margin-left: 40rpx;
}
.icons .item .icon{
  width: 50rpx;
  height: 50rpx;
}
.icons .look{
  width: 160rpx;
  height: 40rpx;
  line-height: 40rpx;
  border-radius: 10rpx;
  text-align: center;
  border: 1rpx solid #fff;
  color: #fff;
  position: fixed;
  right: 20rpx;
  bottom: 30rpx;
}

.pwd-box{
  width: 70%;
  margin: 0 auto;
  
}
.text{
  margin-top: 300rpx;
}
.inputpwd{
  margin-top: 12rpx;
  height: 60rpx;
  line-height: 60rpx;
  border-radius: 10rpx;
  border: 1px solid var(--border);
 padding-left: 30rpx;
 background: #fff;
}
.pwdmask{
  background:var(--background);
  position: fixed;
  top: 0;
  height: 100%;
  width: 100%;
}

.sharebtn{
  width: 48rpx;
  height: 48rpx;
  background: url('data:image/png;base64,')no-repeat center center;
}
.sharebtn::after{
  border: none;
}

.nav {
  width: 100%;
  height: 80rpx;
  background-color: #fff;
}

.scroll-view-demo {
  width: 100%;
  white-space: nowrap;
}

.scroll-view-demo .item {
  width: 120rpx;
  height: 90rpx;
  box-sizing: border-box;
  line-height: 90rpx;
  text-align: center;
  display: inline-block;
}

.scroll-view-demo .active {
  color: var(--border);
  border-bottom: 2rpx solid #ededed;
}